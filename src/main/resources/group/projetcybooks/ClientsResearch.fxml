<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group.projetcybooks.SceneController">
    <children>
        <VBox layoutX="20.0" layoutY="20.0" spacing="10.0">
            <children>
                <Label text="Clients">
                    <font>
                        <Font size="48.0" />
                    </font>
                </Label>
                <TextField fx:id="firstNameField" promptText="First Name" />
                <TextField fx:id="lastNameField" promptText="Last Name" />
                <TextField fx:id="phoneField" promptText="Phone" />
                <Button text="Search" prefWidth="100.0" />
                <Button mnemonicParsing="false" onAction="#switchToClientsMain" text="Back" />
            </children>
        </VBox>
        <Region layoutX="350.0" layoutY="20.0" prefWidth="220.0" prefHeight="360.0" style="-fx-border-color: black;">
            <!-- This region will display the search results -->
        </Region>
    </children>
</AnchorPane>
