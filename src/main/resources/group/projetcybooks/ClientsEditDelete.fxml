<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group.projetcybooks.SceneController">
    <children>
        <VBox layoutX="20.0" layoutY="20.0" spacing="10.0">
            <children>
                <Label text="Edit/Delete">
                    <font>
                        <Font size="48.0" />
                    </font>
                </Label>
                <TextField fx:id="firstNameField" promptText="First Name" />
                <TextField fx:id="lastNameField" promptText="Last Name" />
                <TextField fx:id="phoneField" promptText="Phone" />
                <Button prefWidth="100.0" text="Edit profile" />
                <Button mnemonicParsing="false" onAction="#switchToClientsMain" text="Back" />
            </children>
        </VBox>
        <Region layoutX="350.0" layoutY="20.0" prefHeight="322.0" prefWidth="220.0" style="-fx-border-color: black;">
            <!-- This region will display the search results -->
        </Region>
      <Button layoutX="416.0" layoutY="342.0" mnemonicParsing="false" text="Delete profile" />
    </children>
</AnchorPane>
